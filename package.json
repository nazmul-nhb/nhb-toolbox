{
	"name": "nhb-toolbox",
	"version": "4.11.4",
	"description": "A versatile collection of smart, efficient, and reusable utility functions and classes for everyday development needs.",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"sideEffects": false,
	"scripts": {
		"clean": "rimraf dist",
		"build": "tsup --config tsup.config.ts",
		"prepublishOnly": "tsup --config tsup.config.ts",
		"test": "jest --coverage --verbose",
		"format": "prettier --write .",
		"lint": "node scripts/lint.mjs",
		"fix": "node scripts/fix.mjs",
		"commit": "node scripts/commit.mjs",
		"count": "node scripts/count.mjs",
		"types": "node scripts/types.mjs"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist",
		"LICENSE",
		"README.md",
		"package.json"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nazmul-nhb/nhb-toolbox.git"
	},
	"homepage": "https://nhb-toolbox.vercel.app",
	"author": {
		"name": "Nazmul Hassan",
		"email": "nazmulnhb@gmail.com"
	},
	"license": "Apache-2.0",
	"devDependencies": {
		"@eslint/js": "^9.18.0",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.10.7",
		"@typescript-eslint/eslint-plugin": "^8.21.0",
		"@typescript-eslint/parser": "^8.21.0",
		"chalk": "4.1.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.3",
		"execa": "^9.5.2",
		"globals": "^15.14.0",
		"globby": "^14.0.2",
		"jest": "^29.7.0",
		"prettier": "^3.4.2",
		"progress-estimator": "^0.3.1",
		"rimraf": "^6.0.1",
		"ts-jest": "^29.2.5",
		"tsup": "^8.5.0",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.21.0"
	},
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./types": {
			"import": {
				"types": "./dist/dts/types/index.d.mts",
				"default": "./dist/dts/types/index.d.mts"
			},
			"require": {
				"types": "./dist/dts/types/index.d.ts",
				"default": "./dist/dts/types/index.d.ts"
			}
		},
		"./chronos": {
			"import": {
				"types": "./dist/chronos.d.mts",
				"default": "./dist/chronos.mjs"
			},
			"require": {
				"types": "./dist/chronos.d.ts",
				"default": "./dist/chronos.js"
			}
		}
	},
	"typesVersions": {
		"*": {
			"chronos": [
				"dist/chronos.d.ts"
			]
		}
	},
	"keywords": [
		"toolbox",
		"utility",
		"functions",
		"class",
		"form",
		"random-id",
		"id",
		"parse",
		"format",
		"generate",
		"generator",
		"data",
		"form-data",
		"development",
		"tools",
		"helpers",
		"utils",
		"nhb",
		"color",
		"chronos",
		"time",
		"date-fns",
		"javascript",
		"typescript",
		"moment",
		"dayjs",
		"date",
		"day",
		"nhb-toolbox"
	],
	"packageManager": "pnpm@10.6.2+sha512.47870716bea1572b53df34ad8647b42962bc790ce2bf4562ba0f643237d7302a3d6a8ecef9e4bdfc01d23af1969aa90485d4cebb0b9638fa5ef1daef656f6c1b"
}
